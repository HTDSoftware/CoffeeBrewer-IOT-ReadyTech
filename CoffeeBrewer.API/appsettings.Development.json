{
  "AllowedHosts": "*",
  "ApiUris": {
    "GeolocationApi": "http://api.openweathermap.org/geo/1.0/direct",
    "IpInfoApi": "https://ipinfo.io",
    "WeatherApi": "https://api.openweathermap.org/data/2.5/weather"
  },
  "ConnectionStrings": {
    "RedisConnection": "redis:6379"
  },
  "DefaultLocation": {
    "City": "Melbourne",
    "CountryCode": "AU",
    "State": "VIC"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "MinmumTempForIced": "15",
  "UseRedis": false
}